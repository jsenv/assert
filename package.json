{
  "name": "@jsenv/assert",
  "version": "2.6.0",
  "description": "One assertion to test everything",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/assert"
  },
  "author": {
    "name": "dmail",
    "email": "dmaillard06@gmail.com",
    "url": "https://twitter.com/damienmaillard"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/dist/",
    "/src/"
  ],
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "dev": "node ./script/dev.mjs",
    "test": "node ./script/test.mjs",
    "test-with-coverage": "npm run test -- --coverage",
    "dist": "node ./script/build.mjs",
    "prettier": "prettier --write .",
    "playwright-install": "npx playwright install-deps && npx playwright install",
    "prepublishOnly": "npm run dist"
  },
  "dependencies": {
    "@jsenv/inspect": "1.7.1"
  },
  "devDependencies": {
    "@jsenv/core": "27.0.0-alpha.91",
    "@jsenv/eslint-import-resolver": "0.3.0",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/github-release-package": "1.4.0",
    "@jsenv/package-publish": "1.7.5",
    "eslint": "8.18.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "playwright": "1.22.2",
    "prettier": "2.7.1"
  }
}
